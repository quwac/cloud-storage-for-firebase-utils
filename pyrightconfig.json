{
    "include": [
        "src"
    ],
    "exclude": [
        "**/node_modules",
        "**/__pycache__"
    ],
    "venvPath": ".",
    "venv": ".venv",
    "verboseOutput": true,
    "pythonVersion": "3.6",
    "typeCheckingMode": "strict",
    "reportCallInDefaultInitializer": true,
    "reportImplicitStringConcatenation": true,
    "reportUnusedCallResult": true,
    "executionEnvironments": [
        {
            "root": "./src/",
            "extraPaths": ["./src/"]
        },
        {
            "root": "./tests/",
            "extraPaths": ["./src/"]
        }
    ]
}
